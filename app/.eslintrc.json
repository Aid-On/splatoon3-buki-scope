{
  "extends": ["eslint:recommended", "eslint-config-google", "next/core-web-vitals", "prettier"],
  "rules": {
    "quotes": ["error", "double"],
    "import/no-unresolved": 0,
    "no-unused-vars": "warn",
    "require-jsdoc": 0,
    "@typescript-eslint/no-namespace": "off",
    "quote-props": ["error", "as-needed"],
    "camelcase": "off",
    "new-cap": "off"
  },
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "plugins": ["@typescript-eslint", "import"],
  "env": {
    "jest": true
  }
}
